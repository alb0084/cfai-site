{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": false,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"types": [
			"svelte"
		],
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		".svelte-kit",
		"build"
	]
}